<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:text="Dashboard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/dashboardButton" />
    <Button
        android:text="Load On Demand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/loadOnDemandButton" />
    <Button
        android:text="Email TL Manager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/emailTlManagerButton" />
    <Button
        android:text="Manager Chat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/managerChatButton" />
    <Button
        android:text="Agent Phone/Email Change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/agentPhoneEmailChngButton" />
    <Button
        android:text="Owner Contact Info Change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/ownerContactChangeButton" />
    <Button
        android:text="Distribution Change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/distributionChangeButton" />
    <Button
        android:text="Authorize Simplified Issue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/authorizeSimplifiedIssueButton" />
    <Button
        android:text="User Password Change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/userPasswordChangeButton" />
    <Button
        android:text="Verint Recording Outage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="200dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/verintRecordingOutageButton" />
</LinearLayout>